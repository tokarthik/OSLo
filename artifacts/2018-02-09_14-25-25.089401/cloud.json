{
 "scenarios": {
  "blazedemo": {
   "headers": {
    "Upgrade-Insecure-Requests": "1"
   }, 
   "requests": [
    {
     "think-time": null, 
     "body": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://blazedemo.com/", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "Welcome Page", 
     "headers": {}, 
     "timeout": null, 
     "method": "GET", 
     "keepalive": null
    }, 
    {
     "body": {
      "toPort": "${toPort}", 
      "fromPort": "${fromPort}"
     }, 
     "think-time": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://blazedemo.com/reserve.php", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "Reserve Page", 
     "headers": {}, 
     "timeout": null, 
     "method": "POST", 
     "keepalive": null
    }, 
    {
     "think-time": null, 
     "body": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://blazedemo.com/purchase.php", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "Purchase Page", 
     "headers": {}, 
     "timeout": null, 
     "method": "GET", 
     "keepalive": null
    }, 
    {
     "think-time": null, 
     "body": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://blazedemo.com/confirmation.php", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "Confirmation Page", 
     "headers": {}, 
     "timeout": null, 
     "method": "GET", 
     "keepalive": null
    }
   ], 
   "data-sources": [
    {
     "path": "param.csv", 
     "delimiter": ", ", 
     "variable-names": "fromPort,toPort"
    }
   ], 
   "retrieve-resources": false, 
   "script": null
  }
 }, 
 "settings": {
  "verbose": false
 }, 
 "modules": {
  "gatling": {
   "properties": {
    "gatling.data.file.bufferSize": 256
   }
  }, 
  "grinder": {
   "properties": {
    "grinder.useConsole": false
   }
  }, 
  "consolidator": {
   "rtimes-len": 1000
  }, 
  "jmeter": {
   "properties": {
    "jmeter.save.saveservice.autoflush": "true", 
    "jmeter.save.saveservice.connect_time": "true", 
    "summariser.name": "", 
    "jmeterengine.force.system.exit": "true"
   }
  }
 }, 
 "local-bzt-version": "1.10.5", 
 "services": [
  {
   "run-at": "local", 
   "local": [
    {
     "metrics": [
      "cpu", 
      "mem", 
      "bytes-sent", 
      "bytes-recv", 
      "disk-read", 
      "disk-write", 
      "disk-space", 
      "engine-loop", 
      "conn-all"
     ]
    }
   ], 
   "module": "monitoring"
  }
 ], 
 "execution": [
  {
   "scenario": "blazedemo", 
   "ramp-up": "30s", 
   "locations": {
    "us-east-1": 1, 
    "us-west-1": 2
   }, 
   "concurrency": 200, 
   "executor": "jmeter", 
   "hold-for": "2m"
  }
 ]
}
